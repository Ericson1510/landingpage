<script>

// Sending an API request using jQuery.ajax
var ip = ''; // Current IP
$.ajax({
	method: 'GET',
	contentType: 'application/json',
	url: 'https://ipwhois.pro/json/' + ip + '?key=bdLAz48te6ViLQU1',
	dataType: 'json',
	success: function(json) {
	    // Country code output, field "country_code"

       var currency_symbol = json.currency_symbol;
        var currency_rates = json.currency_rates;
        var currency_code = json.currency_code;
        var country_flag = json.country_flag;
        var city = json.city;
        var country = json.country;

        var precio = 99.99;
        var precioOriginalMitad = 59.99;


        // Si el precio es en dolares le coloco el precio final exacto para que los paises en moneda de dolares no le aparezca el precio con $2 dolares demás
        if(currency_rates==1){
            var PrecioDescuento = 49.99;
            var precioOriginal = currency_symbol+""+new Intl.NumberFormat("en-US").format((currency_rates * precio).toFixed(2))+" "+currency_code;
            var precioMitad = currency_symbol+""+new Intl.NumberFormat("en-US").format((currency_rates * precioOriginalMitad).toFixed(2))+" "+currency_code;
            var precioDescuentoFinal = currency_symbol+""+new Intl.NumberFormat("en-US").format((currency_rates * PrecioDescuento).toFixed(2))+" "+currency_code;

        }else{
          // Si el precio es en una moneda que no tiene dolares, le coloco $2 dolares demás para que tenga margen a la hora de realizar el cambio a moneda local
            var PrecioDescuento = 52.00;
            var precioOriginal = currency_symbol+""+new Intl.NumberFormat("en-US").format((currency_rates * precio).toFixed(0))+" "+currency_code;
            var precioMitad = currency_symbol+""+new Intl.NumberFormat("en-US").format((currency_rates * precioOriginalMitad).toFixed(0))+" "+currency_code;
            var precioDescuentoFinal = currency_symbol+""+new Intl.NumberFormat("en-US").format((currency_rates * PrecioDescuento).toFixed(0))+" "+currency_code;

        }



 $(".PrecioFullTable").html(precioOriginal);
 $(".PrecioFinalTable").html(precioDescuentoFinal);
 $(".precioFinalOfertaCompleta").html(precioDescuentoFinal);
 $(".cityOferta").html(city);
 $(".paisOferta").html(country);
 $(".banderaOferta").attr("src",country_flag);
 $(".paisValorado").html(country);
$("#precioOriginalBotton").html(precioOriginal);
$("#precioDescuentoBotton").html(precioDescuentoFinal);
$(".paisName").html(country);
	}
});

