   <div id="countdown"></div>
   <script>
   var someDate = new Date();
   var dd = someDate.getDate();
   var mm = someDate.getMonth() + 1;
   var y = someDate.getFullYear();
   if(mm<10){
     mm= "0"+mm;
   }

   if(dd<10){
     dd= "0"+dd;
   }
   var newFecha = mm+"/"+dd+"/"+y;

   var horaActual = new Date();
    horaActual.setMinutes(horaActual.getMinutes()+52);
   var horaActualFinal = horaActual.getHours();
   var end = new Date(horaActual);
   //var end = new Date(newFecha+' 11:59 PM'); // fin del dia


       var _second = 1000;
       var _minute = _second * 60;
       var _hour = _minute * 60;
       var _day = _hour * 24;
       var timer;

       function showRemaining() {
           var now = new Date();
           var distance = end - now;
           if (distance < 0) {

               clearInterval(timer);
               document.getElementById('countdown').innerHTML = 'EXPIRED!';

               return;
           }
           var days = Math.floor(distance / _day);
           var hours = Math.floor((distance % _day) / _hour);
           var minutes = Math.floor((distance % _hour) / _minute);
           var seconds = Math.floor((distance % _minute) / _second);

/*  Para el reloj en bottom
           document.getElementById('horasShow2').innerHTML = hours;
           document.getElementById('minutosShow2').innerHTML = minutes;
           document.getElementById('segundosShow2').innerHTML = seconds;*/


           document.getElementById('horasShow').innerHTML = hours;
           document.getElementById('minutosShow').innerHTML = minutes;
           document.getElementById('segundosShow').innerHTML = seconds;
       }

       timer = setInterval(showRemaining, 1000);

   </script>


</body>

